  -webkit-animation: spinner-fade-out 1s linear infinite;
  animation: spinner-fade-out 1s linear infinite;
}

.spinner-crescent circle {
  fill: transparent;
  stroke-width: 4px;
  stroke-dasharray: 128px;
  stroke-dashoffset: 82px;
}

